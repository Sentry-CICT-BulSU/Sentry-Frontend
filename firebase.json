{
  "hosting": [
    {
      "target": "sentry-frontend",
      "public": "dist/sentry-frontend",
      "ignore": ["**/.*"],
      "headers": [
        {
          "source": "*.[0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f].+(css|js)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public,max-age=31536000,immutable"
            }
          ]
        },
        {
          "source": "/@(ngsw-worker.js|ngsw.json)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "frameworksBackend": {
        "region": "asia-east1"
      }
    }
  ],
  "deploy": {
    "builder": "@angular/fire:deploy",
    "options": {
      "prerender": false,
      "ssr": false,
      "browserTarget": "sentry-frontend:build:production",
      "firebaseProject": "cict-sentry",
      "firebaseHostingSite": "cict-sentry",
      "serverTarget": "sentry-frontend:server:production",
      "prerenderTarget": "sentry-frontend:prerender:production"
    }
  }
}
