{
  "name": "sentry-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/sentry-frontend/server/main.js",
    "start-server": "npm run build:ssr && npm run serve:ssr && node dist/sentry-frontend/server/main.js",
    "build": "ng build --configuration=production --verbose",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-high-memory": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration=prod",
    "dev:ssr": "ng run sentry-frontend:serve-ssr",
    "serve:ssr": "node dist/sentry-frontend/server/main.js",
    "build:ssr": "ng build && ng run sentry-frontend:server",
    "prerender": "ng run sentry-frontend:prerender"
  },
  "private": true,
